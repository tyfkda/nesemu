{
  "name": "nesemu",
  "version": "0.9.0",
  "description": "NES Emulator",
  "main": "index.js",
  "scripts": {
    "start": "npm run default",
    "default": "gulp",
    "build": "gulp build",
    "watch": "gulp watch",
    "server": "webpack-dev-server --content-base public",
    "test": "gulp test",
    "lint": "gulp lint",
    "lint:fix": "eslint --fix src/**/*.ts",
    "release": "gulp release",
    "clean": "gulp clean",
    "deploy": "pushd gh-pages && git reset --hard && git checkout gh-pages && popd && npm run release && cp -r release/* gh-pages/ && pushd gh-pages && git commit -a --amend -m 'Deploy' && git push -f && popd"
  },
  "author": "tyfkda",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyfkda/nesemu"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/core": "~7.11.4",
    "@babel/preset-env": "~7.11.0",
    "@babel/register": "~7.10.3",
    "@types/jest": "~24.0.25",
    "@types/node": "~14.6.0",
    "@typescript-eslint/eslint-plugin": "~4.7.0",
    "@typescript-eslint/parser": "~4.7.0",
    "argv": "~0.0.2",
    "browser-sync": "~2.26.7",
    "clone": "~2.1.2",
    "del": "~6.0.0",
    "eslint": "~7.13.0",
    "gulp": "~4.0.2",
    "gulp-cssnano": "~2.1.3",
    "gulp-ejs": "~5.1.0",
    "gulp-eslint": "~6.0.0",
    "gulp-htmlmin": "~5.0.1",
    "gulp-jest": "~4.0.3",
    "gulp-plumber": "~1.2.1",
    "gulp-sass": "~4.1.0",
    "gulp-util": "~3.0.8",
    "jest": "~24.9.0",
    "jest-cli": "~24.9.0",
    "svg-inline-loader": "0.8.2",
    "ts-jest": "~24.3.0",
    "ts-loader": "~8.0.3",
    "ts-node": "~9.0.0",
    "typescript": "~4.0.2",
    "webpack": "~4.44.1",
    "webpack-stream": "~5.2.1"
  },
  "dependencies": {
    "core-js": "~3.6.5",
    "escape-html": "~1.0.3",
    "fscreen": "~1.0.2",
    "jszip": "~3.5.0",
    "md5": "~2.3.0",
    "stats-js": "~1.0.1"
  }
}
