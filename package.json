{
  "name": "nesemu",
  "version": "0.9.0",
  "description": "NES Emulator",
  "main": "index.js",
  "scripts": {
    "start": "npm run default",
    "default": "gulp",
    "build": "gulp build",
    "watch": "gulp watch",
    "server": "webpack-dev-server --content-base public",
    "test": "gulp test",
    "lint": "gulp lint",
    "lint:fix": "eslint --fix src/**/*.ts",
    "release": "gulp release",
    "clean": "gulp clean",
    "deploy": "npm run reset-ghpages && npm run update-ghpages && npm run push-ghpages",
    "reset-ghpages": "cd gh-pages && git reset --hard && git checkout gh-pages && cd ..",
    "update-ghpages": "npm run release && cp -r release/* gh-pages/",
    "push-ghpages": "cd gh-pages && git commit -a --amend -m 'Deploy' && git push -f && cd .."
  },
  "author": "tyfkda",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyfkda/nesemu"
  },
  "engines": {
    "node": ">=16.0"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/preset-env": "~7.16.4",
    "@babel/register": "~7.16.0",
    "@types/fscreen": "~1.0.1",
    "@types/jest": "~27.0.3",
    "@types/md5": "~2.3.1",
    "@types/node": "~16.11.11",
    "@typescript-eslint/eslint-plugin": "~5.5.0",
    "@typescript-eslint/parser": "~5.5.0",
    "argv": "~0.0.2",
    "browser-sync": "~2.27.12",
    "clone": "~2.1.2",
    "eslint": "~8.4.0",
    "gulp": "~4.0.2",
    "gulp-cssnano": "~2.1.3",
    "gulp-ejs": "~5.1.0",
    "gulp-eslint": "~6.0.0",
    "gulp-htmlmin": "~5.0.1",
    "gulp-jest": "~4.0.4",
    "gulp-sass": "~5.0.0",
    "gulp-util": "~3.0.8",
    "html-loader": "~3.0.1",
    "jest": "~27.4.3",
    "jest-cli": "~27.4.3",
    "sass": "~1.44.0",
    "svg-inline-loader": "0.8.2",
    "ts-jest": "~27.0.7",
    "ts-loader": "~9.2.6",
    "ts-node": "~10.4.0",
    "typescript": "~4.5.2",
    "webpack": "~5.64.4",
    "webpack-stream": "~7.0.0"
  },
  "dependencies": {
    "core-js": "~3.19.2",
    "escape-html": "~1.0.3",
    "fscreen": "~1.2.0",
    "jszip": "~3.8.0",
    "md5": "~2.3.0",
    "stats-js": "~1.0.1",
    "stream": "~0.0.2"
  }
}
